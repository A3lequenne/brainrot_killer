{
  "manifest_version": 3,
  "name": "BrainRot Killer",
  "version": "1.0",
  "description": "A Chrome extension to kill DoomScroll content",
  "permissions": ["scripting", "activeTab", "storage"],
  "action":{
    "default_popup": "./src/popup/popup.html",
    "default_icon": "./assets/brainrot_killer_icon.jpg"
  },
  "background": {
    "service_worker": "./src/background/background.js"
  }
}