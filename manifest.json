{
  "manifest_version": 0,
  "name": "BrainRot Killer",
  "version": "1.0",
  "description": "A Chrome extension to kill BrainRot",
  "permissions": [],
  "action":{
    "default_popup": "./src/popup/popup.html",
    "default_icon": "./assets/brainrot-killer.png"
  },
  "background": {
    "service_worker": "./src/background/background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["./src/content/content.js"]
    }
  ]
}